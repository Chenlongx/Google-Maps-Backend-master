{
  "compilerOptions": {
    "target": "ES6", 
    "module": "CommonJS", 
    "moduleResolution": "node",
    "esModuleInterop": true, 
    "strict": true, 
    "skipLibCheck": true, 
    "forceConsistentCasingInFileNames": true, 
    "outDir": "./dist"  // TypeScript 编译输出目录
  },
  "include": [
    "netlify/functions/**/*.ts"  // 编译 TypeScript 文件
  ],
  "exclude": [
    "node_modules"  // 排除 node_modules
  ]
}
